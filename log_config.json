{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "detailed": {
            "format": "[%(levelname)s] %(name)s - %(message)s"
        }
    },
    "handlers": {
        "console": {
            "class": "logging.StreamHandler",
            "level": "INFO",
            "formatter": "detailed",
            "stream": "ext://sys.stdout"
        }
    },
    "root": {
        "level": "INFO",
        "handlers": ["console"]
    },
    "loggers": {
        "src": {
            "level": "INFO",
            "handlers": ["console"],
            "propagate": false
        },
        "rosout": {
            "level": "INFO",
            "handlers": ["console"],
            "propagate": false
        }
    }
}